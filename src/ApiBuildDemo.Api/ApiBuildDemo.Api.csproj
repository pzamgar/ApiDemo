  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\ApiBuildDemo.Core\ApiBuildDemo.Core.csproj"/>
    <ProjectReference Include="..\ApiBuildDemo.Infrastructure\ApiBuildDemo.Infrastructure.csproj"/>
  </ItemGroup>
</Project>